{
  "name": "DiscordTwitterEmbedRX",
  "version": "1.5.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node --env-file=.env ./dist/index.js",
    "start:docker": "node ./dist/index.js",
    "debug": "node --env-file=.env -r ts-node/register ./src/index.ts",
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/index.ts",
    "postinstall": "tsc -p ."
  },
  "lint-staged": {
    "*.@ts": [
      "tsc --noEmit",
      "eslint --ext .ts ./src --fix"
    ],
    "src/**": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t1nyb0x/replyvxtwitter.git"
  },
  "keywords": [
    "discord",
    "twitter"
  ],
  "author": "t1nyb0x",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/t1nyb0x/replyvxtwitter/issues"
  },
  "homepage": "https://github.com/t1nyb0x/replyvxtwitter#readme",
  "devDependencies": {
    "@eslint/compat": "^1.2.1",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@discordjs/builders": "^1.9.0",
    "axios": "^1.7.7",
    "discord.js": "^14.16.3",
    "ts-node": "^10.9.2"
  }
}
